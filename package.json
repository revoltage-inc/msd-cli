{
  "name": "msd-cli",
  "version": "1.0.0",
  "description": "CLI to migrate from Slack to Discord",
  "author": "@yogarasu",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "init": "node --loader ts-node/esm ./command/init.mts",
    "build": "run-s init build:*",
    "build:channel": "node --loader ts-node/esm ./command/build/channel.mts",
    "build:user": "node --loader ts-node/esm ./command/build/user.mts",
    "build:message": "node --loader ts-node/esm ./command/build/message.mts",
    "deploy": "run-s deploy:*",
    "deploy:channel": "node --loader ts-node/esm ./command/deploy/channel.mts",
    "deploy:user": "node --loader ts-node/esm ./command/deploy/user.mts",
    "deploy:message": "node --loader ts-node/esm ./command/deploy/message.mts",
    "destroy": "yes | run-s destroy:*",
    "destroy:user": "node --loader ts-node/esm ./command/destroy/user.mts",
    "destroy:message": "node --loader ts-node/esm ./command/destroy/message.mts",
    "destroy:channel": "node --loader ts-node/esm ./command/destroy/channel.mts",
    "start": "run-s build deploy",
    "update": "npx ncu -u && npm install"
  },
  "volta": {
    "node": "18.7.0",
    "npm": "8.15.1"
  },
  "dependencies": {
    "@discordjs/rest": "^1.0.1",
    "@slack/web-api": "^6.7.2",
    "async-retry": "^1.3.3",
    "commander": "^9.4.0",
    "date-fns": "^2.29.2",
    "discord.js": "^14.3.0",
    "dotenv": "^16.0.1",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.5",
    "@types/node": "^18.7.8",
    "@types/prompts": "^2.0.14",
    "npm-check-updates": "^16.0.5",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
